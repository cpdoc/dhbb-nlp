    "sentences": [
      {
        "text": "«Aral Moreira» nasceu em Mato Grosso no dia 20 de agosto de 1898, filho de Manuel Moreira e de Josefina Trindade Moreira.",
        "location": [
          0,
          121
        ]
      },
      {
        "text": "Bacharelou-se em ciências jurídicas e sociais.",
        "location": [
          122,
          168
        ]
      },
      {
        "text": "Advogado, industrial, comerciante, pecuarista e especialista em cooperativismo, foi subchefe de polícia e promotor público em Mato Grosso e membro da junta governativa do Instituto Nacional do Mate.",
        "location": [
          169,
          367
        ]
      },
      {
        "text": "No pleito de outubro de 1950 elegeu-se deputado federal por seu estado na legenda da União Democrática Nacional (UDN).",
        "location": [
          368,
          486
        ]
      },
      {
        "text": "Assumiu sua cadeira em fevereiro de 1951 e faleceu em novembro de 1952, em pleno exercício do mandato.",
        "location": [
          487,
          589
        ]
      }
    ]
